<h1>Lista de Maestros</h1>

<table class="sortable" id="sortableTable3">
  <tr>
    <th>Nombre</th>
    <th>E-mail</th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to "#{user.first_name} #{user.last_name}", user %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'Editar', edit_user_path(user) %></td>
    <td><%= link_to 'Eliminar', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />


<%= button_to('Nuevo Maestro', new_user_path, :method => :get) %>

