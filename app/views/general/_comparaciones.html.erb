<h1>Lista de Comparaciones</h1>

<table class="sortable" id="tabla">
    <tr>
        <th>Matrícula 1</th>
        <th>Matrícula 2</th>
        <th>Algoritmo 1</th>
        <th>Algoritmo 2</th>
        <th>Algoritmo 3</th>
    </tr>
    <% @similarities.each do |sim|%>
    <tr>
        <td><%= sim.source_code1.student_id%></td>
        <td><%= sim.source_code2.student_id%></td>
        <td><%= sim.algorithm_1 %></td>
        <td><%= sim.algorithm_2 %></td>
        <td><%= sim.algorithm_3 %></td>
    </tr>
    <%end%>
</table>
<br />
<%= link_to "Atras", :back%>
