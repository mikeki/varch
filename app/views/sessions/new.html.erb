<h1>Iniciar sesión</h1>

<%= form_tag sessions_path do %>
  <p>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, params[:email] %>
  </p>
  <p>
    <%= label_tag :Contraseña %><br />
    <%= password_field_tag :password %>
  </p>
  <p class="button"><%= submit_tag "Entrar" %></p>
<% end %>
